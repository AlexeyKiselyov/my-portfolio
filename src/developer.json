    "post-order": {
      "title": "postOrder submit helper",
      "language": "javascript",
      "code": "//postOrder submit helper\nexport default function postOrder(newOrder, setIsLoading, resetForm, dispatch) {\n  setIsLoading(true);\n  axios\n    .post(`/order`, newOrder)\n    .then(result => {\n      if (result.data) {\n        messageAddOrder(result.data.name);\n        dispatch(clearCart());\n        resetForm();\n      }\n    })\n    .catch(error => {\n      messageError(error.response.data.message);\n    })\n    .finally(() => {\n      setIsLoading(false);\n    });\n}\n",
      "createdAt": "2023-02-19T10:20:00.000Z",
